  },
  "homepage": "https://github.com/EkeMinusYou/fastify-template#readme",
  "dependencies": {
    "fastify": "^4.0.0",
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
